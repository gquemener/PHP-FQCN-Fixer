{
    "name": "gquemener/php-fqcn-fixer",
    "type": "application",
    "license": "MIT",
    "description": "Standalone tool that ensures psr-0/psr-4 in your php classes, interfaces and traits.",
    "authors": [
        {
            "name": "Gildas Quéméner",
            "email": "gildas.quemener@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpFQCNFixer\\": "src/"
        }
    },
    "require": {
        "prooph/service-bus": "^6.1",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/stopwatch": "^4.0"
    },
    "require-dev": {
        "beberlei/assert": "^2.8",
        "behat/behat": "^3.4",
        "composer/composer": "1.6.0-RC",
        "phpspec/phpspec": "^4.2"
    },
    "config": {
        "sort-packages": true
    }
}
